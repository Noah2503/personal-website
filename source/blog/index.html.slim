=partial('partials/header')
.container
  -page_articles(blog_name = 'blog').group_by { |a| a.date.strftime('%Y') }.to_a.each do |year, articles|
    .row
      .col-md-12
        h2 =year
        ul
          -articles.each do |article|
            li =link_to article.title, "/#{article.path}"
=partial('partials/footer')
