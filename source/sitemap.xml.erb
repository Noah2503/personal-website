<% pages = sitemap.resources.find_all { |p| p.source_file.match(/\.html/) } %>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% pages.each do |page| %>
    <% next if page.data.noindex %>
    <url>
      <loc>https://charlieegan3.com/<%= page.destination_path.gsub(/\/?index\.html/,"") %></loc>
      <priority><%= page.data.priority || (1 - (page.destination_path.count("/") / 10.0)) %></priority>
      <changefreq><%= page.data.changefreq || "monthly" %></changefreq>
    </url>
  <% end %>
</urlset>
